<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta property="og:title" content="Your daily todo manager" />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="http://yourdailytodo.azurewebsites.net/home/index" />
    <meta property="og:image" content="http://yourdailytodo.azurewebsites.net/Content/images/todomainimg.png" />
    <meta property="og:site_name" content="Your daily todo manager" />
    <meta property="fb:admins" content="100001569201471" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/css/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <link href="http://fonts.googleapis.com/css?family=Lora|Oswald" rel="Stylesheet"
        type="text/css" media="screen" />
</head>
<body>
    <div id="fb-root">
    </div>
    <script>        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s); js.id = id;
            js.src = "//connect.facebook.net/en_US/all.js#xfbml=1";
            fjs.parentNode.insertBefore(js, fjs);
        } (document, 'script', 'facebook-jssdk'));</script>
    <div class="page">
        <header class="head">
            <div id="logindisplay">
                @Html.Partial("_LogOnPartial")
                <div>
                <div class="fb-like" data-href="http://yourdailytodo.azurewebsites.net/home/index" data-send="false" data-layout="button_count" data-width="450" data-show-faces="false" data-font="tahoma"></div>
                </div>
            </div>
            <div class="branding">
            <h1>Your Daily ToDo List</h1>
            </div>
        </header>
        <section id="main">
            @RenderBody()
        </section>
        <footer>
        <p>© nishakant mishra 2013</p> 
        <div class="footerfb">
        <div class="fb-like" data-href="http://yourdailytodo.azurewebsites.net/home/index" data-send="false" data-layout="button_count" data-width="450" data-show-faces="false" data-font="tahoma"></div>
        </div>
        </footer>
    </div>
</body>
</html>
